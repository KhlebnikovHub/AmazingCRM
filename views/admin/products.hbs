
<table class="responsive-table" id = "userTable">
        <thead>
          <tr>
              <th>Категория</th>
              <th>Описание</th>
              <th>Остаток</th>
              <th>Цена</th>
          </tr>
        </thead>
        <tbody>
        {{#each products}}
          <div data-did="{{this.id}}">
            <tr data-id="{{this.id}}">
            <td>{{this.categories_id}}</td>
            <td>{{this.description}}</td>
            <td>{{this.stock}}</td>
            <td>{{this.price}}</td>
            <td><a id="adminEdit">Редактировать</a></td>
          </tr>
          </div>
          {{/each}}
        </tbody>
  
 </table>
<script defer src = "/js/admin/products.js"></script>
